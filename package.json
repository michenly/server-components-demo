{
  "name": "react-notes",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.12.3",
    "@babel/register": "^7.12.1",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "10.0.0",
    "compression": "^1.7.4",
    "concurrently": "^5.3.0",
    "date-fns": "^2.16.1",
    "excerpts": "^0.0.3",
    "express": "^4.17.1",
    "html-webpack-plugin-4": "npm:html-webpack-plugin@4.5.0",
    "html-webpack-plugin-5": "npm:html-webpack-plugin@5.3.2",
    "marked": "^1.2.5",
    "nodemon": "^2.0.6",
    "pg": "^8.5.1",
    "react": "0.0.0-experimental-7ec4c5597",
    "react-dom": "0.0.0-experimental-7ec4c5597",
    "react-error-boundary": "^3.1.0",
    "react-fetch": "0.0.0-experimental-7ec4c5597",
    "react-fs": "0.0.0-experimental-7ec4c5597",
    "react-pg": "0.0.0-experimental-7ec4c5597",
    "react-server-dom-webpack": "0.0.0-experimental-7ec4c5597",
    "resolve": "1.12.0",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.2.0",
    "webpack-cli": "^4.2.0",
    "webpack4": "npm:webpack@4.44.2",
    "webpack5": "npm:webpack@5.53.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "prettier": "1.19.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "concurrently \"npm run server:dev\" \"npm run bundler:webpack5:dev\"",
    "start:prod": "concurrently \"npm run server:prod\" \"npm run bundler:webpack5:prod\"",
    "server:dev": "cross-env NODE_ENV=development nodemon -- --conditions=react-server server",
    "server:prod": "cross-env NODE_ENV=production nodemon -- --conditions=react-server server",
    "bundler:webpack4:dev": "cross-env NODE_ENV=development nodemon -- scripts/build-webpack4.js",
    "bundler:webpack4:prod": "cross-env NODE_ENV=production nodemon -- scripts/build-webpack4.js",
    "bundler:webpack5:dev": "npm run buildPlugin && cross-env NODE_ENV=development nodemon -- scripts/build-webpack5.js",
    "bundler:webpack5:prod": "npm run buildPlugin && cross-env NODE_ENV=production nodemon -- scripts/build-webpack5.js",
    "prettier": "prettier --write **/*.js",
    "seed": "node ./scripts/seed.js",
    "buildPlugin": "tsc --esModuleInterop --target es2017 --moduleResolution node --module commonjs scripts/build-webpack5.ts"
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "build/*"
    ]
  }
}
